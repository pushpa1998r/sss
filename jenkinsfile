node {
    
    stage('maven version') {
	   sh 'mvn --version' 
	}
	stage('Java version') {
	   sh 'java --version' 
	}
	stage('git version') {
	    sh 'git --version'
    }
	stage('git clone') {
	    git branch: 'main', credentialsId: 'pushpa', url: 'https://github.com/pushpa1998r/sss.git'
	}
	stage('maven package') {
	    sh 'mvn package'
	}
	
    
}
